{
  "Logging": {
    "LogLevel": {
      "Default": "Error",
      "Microsoft.Hosting.Lifetime": "Error"
    }

  },
  "ConnectionStrings": {
    "DefaultConnection": "Server=${database_hostname}$;Initial Catalog=${database_name}$;Persist Security Info=False;User ID=${database_username}$;Password=${DATABASE_PASSWORD}$;MultipleActiveResultSets=False;Encrypt=True;TrustServerCertificate=True;Connection Timeout=120;"
  },
  "Discord": {
    "Token": "${DiscordBotToken}$",
    "ClientId": ""

  },
  "CacheSettings": {
    "SlidingExpiration": 2
  }
}

